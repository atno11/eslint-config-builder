{
  "name": "@atno11/eslint-config-builder",
  "version": "1.0.0",
  "private": false,
  "description": "Full typed ESLint config builder.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./validation": {
      "types": "./dist/libs/validation/index.d.ts",
      "default": "./dist/libs/validation/index.js"
    },
    "./validation/errors": {
      "types": "./dist/libs/validation/errors.d.ts",
      "default": "./dist/libs/validation/errors.js"
    },
    "./validation/types": {
      "types": "./dist/libs/validation/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}
